        "# Converter a coluna 'dteday' em objetos de data(Há diferença entre a quantidade de bicicletas alugadas nos dias úteis e não úteis?)\n",
        "df_dict['dteday'] = pd.to_datetime(df_dict['dteday'])\n",
        "\n",
        "# Definir uma função para classificar os dias em dias úteis (1) ou não úteis (0)\n",
        "def is_weekday(date):\n",
        "    return date.weekday() < 5  # Segunda a sexta-feira são dias úteis (0-4)\n",
        "\n",
        "# Aplicar a função para criar uma nova coluna 'dia_util' no DataFrame\n",
        "df_dict['dia_util'] = df_dict['dteday'].apply(is_weekday)\n",
        "\n",
        "# Calcular a média de aluguéis para dias úteis e não úteis\n",
        "media_dias_util = df_dict[df_dict['dia_util'] == True]['cnt'].mean()\n",
        "media_nao_dias_util = df_dict[df_dict['dia_util'] == False]['cnt'].mean()\n",
        "\n",
        "# Imprimir os resultados\n",
        "print(\"Média de aluguéis nos dias úteis:\", media_dias_util)\n",
        "print(\"Média de aluguéis nos dias não úteis:\", media_nao_dias_util)\n",
        "\n",
        "# Criar um gráfico de barras para visualizar a média de aluguéis nos dias úteis e não úteis\n",
        "plt.figure(figsize=(8, 5))\n",
        "plt.bar(['Dias Úteis', 'Dias Não Úteis'], [media_dias_util, media_nao_dias_util])\n",
        "plt.xlabel('Dias da Semana')\n",
        "plt.xlabel('Dias da Semana')\n",
        "plt.ylabel('Média de Aluguéis de Bicicletas')\n",
        "plt.title('Média de Aluguéis de Bicicletas nos Dias Úteis e Não Úteis')\n",
        "plt.show()"
      ]
